{
  "framework": "nextjs",
  "functions": {
    "api/market-data/update.ts": {
      "maxDuration": 180
    },
    "api/market-data/auto-update.ts": {
      "maxDuration": 180
    }
  },
  "crons": [
    {
      "path": "/api/market-data/auto-update",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/generate-daily-quiz",
      "schedule": "0 22 * * *"
    },
    {
      "path": "/api/cron/pay-quiz-rewards",
      "schedule": "0 * * * *"
    }
  ]
}